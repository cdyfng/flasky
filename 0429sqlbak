alembic_version  follows          roles          
baseinfo         history          stock          
comments         posts            users          
PRAGMA foreign_keys=OFF;
BEGIN TRANSACTION;
CREATE TABLE baseinfo (
	stock_id VARCHAR(10) NOT NULL, 
	stock_name TEXT, 
	timestamp VARCHAR(20), 
	stock_type TEXT, 
	total_capital FLOAT, 
	total_value FLOAT, 
	pe_ratio_static FLOAT, 
	pe_ratio_dynamic FLOAT, 
	pb FLOAT, 
	income FLOAT, 
	industry TEXT, 
	main_busyness TEXT, 
	concept TEXT, 
	now_price FLOAT, 
	volumn FLOAT, 
	PRIMARY KEY (stock_id), 
	UNIQUE (stock_id, pe_ratio_static, pe_ratio_dynamic, pb)
);
